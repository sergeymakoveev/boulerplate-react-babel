{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    //"plugins": [ flowtype" ],
    "globals": {
        "ymaps": false
    },
    "rules": {
        "strict": 1,
        "quotes": [ 1, "single" ],
        "eol-last": 1,
        "new-cap": 0,
        "no-console": 0,
        "no-debugger": 1,
        "no-extra-semi": 1,
        "no-underscore-dangle": 0,
        "no-unreachable": 2,
        "no-unused-vars": 0, // 1, временно
        "no-use-before-define": 0
    }
}